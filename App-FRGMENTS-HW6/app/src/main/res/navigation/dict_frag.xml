<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dictionary_navigation"
    app:startDestination="@id/dictFragment">
    <fragment
        android:id="@+id/dictFragment"
        android:name="example.myapplication.fragments.DictionaryFragment"
        android:label="DictFragment"
        tools:layout="@layout/fragment_view">
        <action
            android:id="@+id/action_dictFragment_self"
            app:destination="@id/dictFragment" />
        <argument
            android:name="count"
            android:defaultValue="0"
            app:argType="integer" />

    </fragment>
</navigation>